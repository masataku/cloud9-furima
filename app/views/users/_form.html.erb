<%= form_for @user do |f| %>
  <%= f.label :image, "画像" %>
  <%= f.file_field :image %>

  <%= f.label :name, "ユーザー名" %>
  <%= f.text_field :name %>
  
  <%= f.label :email, "メールアドレス" %>
  <%= f.text_field :email %>
  
  <%= f.label :password, "パスワード" %>
  <%= f.password_field :password %>
  
  <%= f.submit "完了" %>
  
<% end %>