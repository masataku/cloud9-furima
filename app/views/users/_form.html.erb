<%= form_for @user do |f| %>

  <p>
    <%= f.label :image, "画像" %><br>
    <%= f.file_field :image %>
  </p>
  <p>
    <%= f.label :name, "ユーザー名" %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :email, "メールアドレス" %><br>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password, "パスワード" %><br>
    <%= f.password_field :password %>
  </p>
  <%= f.submit "登録する" %>
  
<% end %>